{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "ConnectionStrings": {
    "films": "https://{account_name}.blob.core.windows.net/",
    "eventbus-cinema": "aspire-cinema.servicebus.windows.net"
  },
  "Aspire": {
    "Azure": {
        "Storage": {
          "Blobs": {
            "DisableHealthChecks": true,
            "DisableTracing": false,
            "ClientOptions": {
              "Diagnostics": {
                "ApplicationId": "myapp"
              }
            }
          }
        }
      },
      "Microsoft": {
        "EntityFrameworkCore": {
          "SqlServer": {
            "ConnectionString": "YOUR_CONNECTIONSTRING",
            "DbContextPooling": true,
            "DisableHealthChecks": true,
            "DisableTracing": true,
            "DisableMetrics": false
          },
          "Cosmos": {
            "ConnectionString": "YOUR_CONNECTIONSTRING",
            "DbContextPooling": true,
            "DisableTracing": true
          }
        }
      }
    }
}
